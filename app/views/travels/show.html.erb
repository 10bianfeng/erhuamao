var modal_slides=function(a){
	if($('#popout_modal').size()>0){
		$('#popout_modal').remove();
	}
	var $popout_modal_html='<div id="popout_modal" class="popout_modal"><div class="close_modal" onclick="modal_close();">X</div><img src="/images/ajax-loader.gif" id="ajaxloader"></div><div id="popout_modal_mask"></div>';
	$('body').append($popout_modal_html);
	$('#popout_modal').show();
	$.ajax({
			url: '/test.html',
			type: 'GET',
			data: {},
			success: function (data) {
				$('#ajaxloader').remove();
				$('#popout_modal').append(data);
			},
			error:function(){
				$('#ajaxloader').remove();
				$('<p>Someting goes wrong, please try again later.</p>').appendTo('#popout_modal');
